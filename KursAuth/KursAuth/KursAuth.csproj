<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="**\*.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
    <AvaloniaResource Include="**\*.xaml">
      <SubType>Designer</SubType>
    </AvaloniaResource>
    <AvaloniaResource Include="Assets\**" />
  </ItemGroup>
  <ItemGroup>
    <AvaloniaResource Remove="Views\MyFriends.xaml" />
    <AvaloniaResource Remove="Views\MyMessages.xaml" />
    <AvaloniaResource Remove="Views\VkMain.xaml" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Views\MyFriends.xaml" />
  </ItemGroup>
  <ItemGroup>
    <AvaloniaResource Remove="Views\RegControl.xaml" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Views\MyMessages.xaml" />
    <None Remove="Views\RegControl.xaml" />
  </ItemGroup>
  <ItemGroup>
  <!--  <None Remove="Views\MainWindow.xaml" /> -->
    <None Remove="Views\RegControl.xaml" />
    <None Remove="Views\VkMain.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Avalonia" Version="0.9.0" />
    <PackageReference Include="Avalonia.Desktop" Version="0.9.0" />
    <PackageReference Include="Avalonia.ReactiveUI" Version="0.9.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="VkNet" Version="1.50.0" />
    <PackageReference Include="VkNet.AudioBypassService" Version="1.5.3" />
    <PackageReference Include="VkNet" Version="1.50.0" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\MyFriends.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\RegControl.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\VkMain.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\MyMessages.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Views\RegControl.xaml.cs">
      <DependentUpon>RegControl.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>
